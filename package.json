{
  "name": "saucedemo-playwright-tests",
  "version": "1.0.0",
  "description": "Comprehensive E2E test suite for Sauce Labs demo e-commerce website using Playwright",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "install:browsers": "playwright install",
    "codegen": "playwright codegen https://www.saucedemo.com"
  },
  "keywords": ["playwright", "e2e", "testing", "saucedemo", "automation"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.0.0"
  }
}
